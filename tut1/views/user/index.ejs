<div class="container">
	<h3>ALl User </h3>
	<table class="table">
			<tr>
				<th>ID</th>
				<th>Name</th>
				<th>Title</th>
				<th>Email</th>
				<th></th>
				<th></th>
				<th></th>
			</tr>
			<% _.each(user ,function(user){ %>
			<tr data-id="<%= user.id %>" data-model="user">
				<td><%= user.id %></td>
				<td><%= user.name %></td>
				<td><%= user.title %></td>
				<td><%= user.email %></td>	
				<td><a href="/user/show/<%= user.id %>" class="btn btn-sm btn-primary">SHOW</a></td>
				<td><a href="/user/edit/<%= user.id %>" class="btn btn-sm btn-primary">EDIT</a></td>
				<td><a href="/user/destroy/<%= user.id %>" class="btn btn-sm btn-primary">DELETE</a></td>
			</tr>
			<% }) %>
	</table>
</div>